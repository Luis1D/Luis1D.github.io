{
    "version": 2,
    "name": "blog-template",
    "builds": [{ "src": "build/**", "use": "@now/static"}],
    "routes": [
        { "src": "/", "dest": "build/index.html" },
        { "src": "/service-worker.js", "dest": "build/service-worker.js", "headers": { "cache-control": "no-cache" } },
        { "src": "/(.+)", "dest": "build/$1" }
    ]
}